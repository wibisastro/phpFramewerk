<?xml version="1.0" encoding="UTF-8"?>
<list>
	<route>
		<method>GET</method>
		<uri>/js/{app}/{component}</uri>
		<handler>Gov2lib/helper/js</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/img/{app}/{file}</uri>
		<handler>Gov2lib/helper/img</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/</uri>
		<handler>Home</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/index.php</uri>
		<handler>Home</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/opd</uri>
		<handler>OPD</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/opd/{cmd}</uri>
		<handler>OPD</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/opd/{cmd}/{no:\d+}/{opd_id:\d+}</uri>
		<handler>OPD</handler>
	</route>
	<route>
		<method>POST</method>
		<uri>/opd</uri>
		<handler>OPD</handler>
	</route>
	<route>
		<method>GET</method>
		<uri>/phpvb</uri>
		<handler>phpVB</handler>
	</route>
